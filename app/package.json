{
  "name": "holotagger",
  "version": "0.0.3",
  "description": "Application for downloading covers",
  "scripts": {
    "prod": "esbuild src/main.js --bundle --minify --target=chrome58,firefox57,safari11,edge16 --outfile=static/bundle.js",
    "dev": "esbuild src/main.js --bundle --watch --minify --sourcemap=inline --target=chrome58,firefox57,safari11,edge16 --outfile=static/bundle.js",
    "format": "prettier --tab-width 4 -w src/*.js"
  },
  "author": "Nick Yu",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.14.18",
    "prettier": "^2.5.1"
  }
}
